{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$i18n/*": [
        "src/i18n/*"
      ],
      "$components/*": [
        "src/components/*"
      ],
      "$svgs/*": [
        "src/svgs/*"
      ],
      "$styles/*": [
        "src/styles/*"
      ],
      "$scripts/*": [
        "src/scripts/*"
      ]
    },
  },
}